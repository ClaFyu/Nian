<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>AdminLoginServlet</servlet-name>
        <servlet-class>com.zte.servlet.AdminLoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminLoginServlet</servlet-name>
        <url-pattern>/login.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>DrawServlet</servlet-name>
        <servlet-class>com.zte.servlet.DrawServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DrawServlet</servlet-name>
        <url-pattern>/draw.do</url-pattern>
    </servlet-mapping>
    
    <servlet>
        <servlet-name>AddAwardServlet</servlet-name>
        <servlet-class>com.zte.servlet.AddAwardServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AddAwardServlet</servlet-name>
        <url-pattern>/addaward.do</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>JumpServlet</servlet-name>
        <servlet-class>com.zte.servlet.JumpServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JumpServlet</servlet-name>
        <url-pattern>/jump.do</url-pattern>
    </servlet-mapping>
</web-app>